{
  "compilerOptions": {
    "target": "es6",
    "types": [
      "es6-collections",
      "node"
    ],
    "lib": ["es2017.object", "es2016"],
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  }
}
