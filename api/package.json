{
  "name": "hackpsu-api",
  "version": "0.2.1",
  "engines": {
    "node": "~8"
  },
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "nodemon ./dist/bin/www",
    "start": "node ./dist/bin/www",
    "watch": "nodemon ./dist/bin/www",
    "test": "nyc mocha",
    "prod": "APP_ENV=prod npnm run build && npm start",
    "apidoc": "apidoc -i ./lib/routes -o doc/",
    "debug": "node --inspect ./dist/bin/www"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "lines": 80,
    "branches": 50,
    "functions": 80,
    "cache": true,
    "exclude": [
      "**/*.d.ts",
      "**/*.js",
      "test/**/*"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "@fluffy-spoon/substitute": "1.0.66",
    "@google-cloud/debug-agent": "2.6.0",
    "@google-cloud/logging-bunyan": "0.9.5",
    "@google-cloud/monitoring": "0.5.0",
    "@google-cloud/storage": "1.7.0",
    "@google-cloud/trace-agent": "2.10.1",
    "@sendgrid/mail": "6.3.1",
    "@types/bunyan": "1.8.5",
    "@types/chance": "1.0.1",
    "@types/express": "4.16.0",
    "@types/mocha": "5.2.5",
    "@types/multer": "1.3.7",
    "@types/mysql": "2.15.5",
    "@types/uuid": "3.4.4",
    "ajv": "^5.5.2",
    "body-parser": "1.18.3",
    "bunyan": "1.8.12",
    "chai": "4.2.0",
    "chai-http": "4.2.0",
    "chance": "1.0.16",
    "cookie-parser": "~1.4.3",
    "cors": "2.8.5",
    "dotenv": "6.0.0",
    "email-validator": "1.2.3",
    "express": "4.16.3",
    "firebase": "5.5.5",
    "firebase-admin": "5.12.0",
    "helmet": "3.12.1",
    "injection-js": "2.2.1",
    "lodash": "4.17.10",
    "mailchimp-api-v3": "1.12.0",
    "mocha": "4.1.0",
    "multer": "1.4.1",
    "multer-google-storage": "1.2.1",
    "mysql": "2.15.0",
    "node-time-uuid": "0.1.5",
    "pug": "2.0.3",
    "reflect-metadata": "0.1.12",
    "request": "2.86.0",
    "rxjs": "6.3.3",
    "sinon": "7.2.2",
    "sinon-express-mock": "2.1.0",
    "squel": "5.12.2",
    "streaming-json-stringify": "3.1.0",
    "source-map-support": "0.5.9",
    "ts-stream": "2.0.0",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "uuid": "3.2.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.117",
    "apidoc": "0.17.7",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-json": "1.2.0",
    "grunt": "1.0.3",
    "grunt-exec": "3.0.0",
    "grunt-run": "0.8.1",
    "mocha": "4.1.0",
    "nodemon": "1.18.4",
    "nyc": "13.1.0",
    "ts-node": "7.0.1",
    "typings": "2.1.1"
  },
  "apidoc": {
    "name": "HackPSU RESTful Web API",
    "version": "1.0.0",
    "description": "RESTFul API for all HackPSU connections",
    "url": "https://api.hackpsu.org/v1",
    "header": {
      "title": "HackPSU RESTful Web API",
      "file": "header.md"
    }
  }
}
